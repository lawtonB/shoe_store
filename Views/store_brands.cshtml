<h2>@Model["store"].GetStore_Name()'s brands</h2>
@if (@Model["brands"].Count == 0)
{
  <p>this story carries no brands</p>
}
else
{
  <ol>
    @foreach (var brand in @Model["brands"])
    {
      <li>
        @brand.GetBrand_Name()
      </li>
    }
  </ol>
}
<h4><a href="/">Return Home</a></h4>
<h4><a href="/brands">Return to Brands</a></h4>
<h4><a href="/stores">View Stores</a></h4>
